<div class="row">
    <div class="col-lg-12">
        <div class="m-b-md">
            <div class="">

            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-lg-5">
        <dl class="dl-horizontal">

            <dt><%= t('activerecord.attributes.client.application_number') %>:</dt>
            <dd><%= @client.application_number %></dd>
            <dt><%= t('activerecord.attributes.client.id_number_type') %>:</dt>
            <dd><%= Client.id_number_type_names[@client.id_number_type] %></dd>
            <dt><%= t('activerecord.attributes.client.id_number') %>:</dt>
            <dd><%= @client.id_number %> </dd>
            <dt><%= t('activerecord.attributes.client.sales_man_code') %>:</dt>
            <dd> <%= @client.sales_man_code %></dd>
        </dl>
    </div>
    <div class="col-lg-7">
        <dl class="dl-horizontal">

            <dt><%= t('activerecord.attributes.client.traded_at') %>:</dt>
            <dd><%= @client.traded_at.strftime('%Y-%m-%d %H:%M:%S') if @client.traded_at %></dd>
            <dt><%= t('activerecord.attributes.client.fund_trading_account_number') %>:</dt>
            <dd> <%= @client.fund_trading_account_number %></dd>
            <dt><%= t('activerecord.attributes.client.fund_account_number') %>:</dt>
            <dd> <%= @client.fund_account_number %></dd>
        </dl>
    </div>
</div>

<div class="row">
    <div class="col-lg-5">
        <dl class="dl-horizontal">

            <dt><%= t('activerecord.attributes.client.person_in_charge_name') %>:</dt>
            <dd><%= @client.person_in_charge_name %></dd>
            <dt><%= t('activerecord.attributes.client.person_in_charge_id_number_type') %>:</dt>
            <dd><%= Client.id_number_type_names[@client.person_in_charge_id_number_type] %></dd>
            <dt><%= t('activerecord.attributes.client.person_in_charge_id_number') %>:</dt>
            <dd><%= @client.person_in_charge_id_number %> </dd>
        </dl>
    </div>
    <div class="col-lg-7">
        <dl class="dl-horizontal">


        </dl>
    </div>
</div>
