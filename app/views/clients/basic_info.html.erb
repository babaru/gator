<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2><%= t('activerecord.text.instances_list', model: Client.model_name.human) %></h2>
        <ol class="breadcrumb">
            <li>
                <%= link_to t('text.home'), root_path %>
            </li>
            <li class="active">
                <strong><%= t('activerecord.text.instances_list', model: Client.model_name.human) %></strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2"></div>
</div>

<div class="wrapper wrapper-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-title">
                    <h5>客户开户向导</h5>
                </div>
                <div class="ibox-content">

                    <div role="application" class="wizard clearfix">
                        <div class="steps clearfix">
                            <ul role="tablist">
                                <li role="tab" class="first disabled">
                                    <a id="wizard-t-0" href="#" aria-controls="wizard-p-0"><span class="current-info audible">current step: </span><span class="number">1.</span> First Step</a>
                                </li>
                                <li role="tab" class="current">
                                    <a id="wizard-t-1" href="#wizard-h-1" aria-controls="wizard-p-1"><span class="number">2.</span> Second Step</a>
                                </li>
                                <li role="tab" class="disabled last" aria-disabled="true">
                                    <a id="wizard-t-2" href="#wizard-h-2" aria-controls="wizard-p-2"><span class="number">3.</span> Third Step</a>
                                </li>
                            </ul>
                        </div><div class="content clearfix">

                          <div class="step-content body current" id="wizard-p-0" role="tabpanel" aria-labelledby="wizard-h-0" aria-hidden="false">
                              <div class="text-center m-t-md">
                                <h2>选择客户类型</h2>
                                <p>
                                    <%= render 'form' %>
                                </p>
                              </div>
                          </div>
                    </div>

                    <div class="actions clearfix">
                        <ul role="menu" aria-label="Pagination">
                            <li class="disabled" aria-disabled="true">
                                <a href="#previous" role="menuitem">Previous</a>
                            </li>
                            <li aria-hidden="false" aria-disabled="false">
                                <a href="#next" role="menuitem">Next</a>
                            </li>
                            <li aria-hidden="false">
                                <a href="#finish" role="menuitem">Finish</a>
                            </li>
                            <li>
                                <a href="#cancel" role="menuitem">Cancel</a>
                            </li>
                        </ul>
                    </div>


                </div>
            </div>
        </div>
    </div>
</div>
